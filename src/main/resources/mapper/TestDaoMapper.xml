<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sun.testproject.dao.TestDao">
    <!-- 根据主键查询-->
    <select id="getById" resultType="com.sun.testproject.entity.TestEntity" parameterType="java.lang.Integer" >
        select  *
        from t_student
        where id = #{id}
    </select>
</mapper>